<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="289.0" prefWidth="288.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.BuildingController">
   <children>
      <GridPane layoutX="28.0" layoutY="27.0" prefHeight="255.0" prefWidth="232.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Building Id:" />
            <Label text="Address:" GridPane.rowIndex="1" />
            <Label text="City:" GridPane.rowIndex="2" />
            <Label text="State:" GridPane.rowIndex="3" />
            <Label text="Zip Code:" GridPane.rowIndex="4" />
            <Label text="Units:" GridPane.rowIndex="5" />
            <Label text="Updated:" GridPane.rowIndex="6" />
            <TextField fx:id="buildingIdTextField" disable="true" editable="false" GridPane.columnIndex="1" />
            <TextField fx:id="addressIdTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="cityTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="stateTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="zipTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="unitsTextField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="updatedTextField" disable="true" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="7">
               <children>
                  <Button fx:id="building1Button" mnemonicParsing="false" onAction="#building1ButtonClicked" text="Building 1" />
                  <Button fx:id="building2Button" mnemonicParsing="false" onAction="#building2ButtonClicked" text="Building 2" />
                  <Button fx:id="building3Button" mnemonicParsing="false" onAction="#building3ButtonClicked" text="Building 3" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
